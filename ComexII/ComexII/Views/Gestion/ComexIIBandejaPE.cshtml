    <meta name="author" content="Sermaluc S.A." />

    @Styles.Render("~/Content/css")

    <style type="text/css">
        .modal {
            display: none;
            position: fixed;
            z-index: 1000;
            top: 0;
            left: 0;
            height: 100%;
            width: 100%;
            background: #ACB4B9 url('/pu46/pux/ComexIIConsultas/Content/jqueryui1114/images/page-loader.gif') 50% 50% no-repeat;
            -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
        }

        body.loading .modal {
            overflow: hidden;
        }

        body.loading .modal {
            display: block;
        }

        .table2 {
            border: 1px solid #ACB4B9;
            background-color: #FFFFFF;
            font-family: Verdana;
            font-size: 9px;
        }

        .table3 {
            border: 0px;
            background-color: #FFFFFF;
            font-family: Verdana;
            font-size: 9px;
        }

        .TDBorder {
            border-color: #ACB4B9;
            border-style: solid;
            border-width: 1px;
        }
    </style>

</head>

