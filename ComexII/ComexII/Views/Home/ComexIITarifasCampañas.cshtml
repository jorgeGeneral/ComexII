@{
    ViewBag.Title = "Tarifas para Campañas";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=11" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="viewport" content="initial-scale=1.0, maximum-scale=2.0" />
    <link rel="shortcut icon" href="" />
    <meta name="description" content="Comex II - Matriz de Tarifado" />
    <meta name="author" content="Sermaluc S.A." />


    @Styles.Render("~/Content/css")

    <style type="text/css">
        .modal {
            display: none;
            position: fixed;
            z-index: 1000;
            top: 0;
            left: 0;
            height: 100%;
            width: 100%;
            background: #ACB4B9 url('/pu46/pux/ComexIIConsultas/Content/jqueryui1114/images/page-loader.gif') 50% 50% no-repeat;
            -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
        }

        body.loading .modal {
            overflow: hidden;
        }

        body.loading .modal {
            display: block;
        }

        .table2 {
            border: 1px solid #ACB4B9;
            background-color: #FFFFFF;
            font-family: Verdana;
            font-size: 9px;
        }

        .table3 {
            border: 0px;
            background-color: #FFFFFF;
            font-family: Verdana;
            font-size: 9px;
        }

        .TDBorder {
            border-color: #ACB4B9;
            border-style: solid;
            border-width: 1px;
        }
    </style>
</head>

<body>
    <center>
        <div id="wait" class="modal"></div>

        <div id="dialog-message" title="Tarifas para Campañas" style="display: none">
            <br />
            <strong>
                <label class="labelBE" id="lblMsg" style="text-align: center"></label>
            </strong>
        </div>

        <div id="dialog-error" title="Error" style="display: none">
            <br />
            <strong>
                <label class="labelBE2" id="lblError" style="text-align: center;">
                </label>
            </strong>
            <br />
            <div id="myDIV" style="display: none">
                <code id="error1"></code>
                <br />
                <code id="error2"></code>
                <br />
                <code id="error3"></code>
            </div>
        </div>

        <div id="divFiltrosODP">
            <table style="width: 100%">
                <tr>
                    <td>
                        <div class="Titulo1BE">
                            Tarifas para Campañas
                        </div>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <a href="#"><img src="~/Content/Images/excel-icon.png" alt="Exportar" height="20" width="20">&nbsp;exportar</a>&nbsp;&nbsp;&nbsp;&nbsp;
                        <a href="#"><img src="~/Content/Images/print-icon.png" alt="Imprimir" height="20" width="20">&nbsp;imprimir</a>
                    </td>
                </tr>
            </table>

            <table style="width: 100%; border-top: 5px">
                <tr>
                    <td colspan="3">
                        &nbsp;
                    </td>
                </tr>
                <tr>
                    <td width="20%">
                        <img src="~/Content/Images/flecha.jpg" style="width:12px;height:12px" />&nbsp;<b><label class="labelBE">Tipo Producto: </label></b>
                    </td>
                    <td width="30%">
                        <div class="custom-select" style="width: 90%; float: left">
                            <select id="cboTipoProducto" onchange="cambiaGrillaTarifa()">
                                <option value="1" selected="selected">ODP al Exterior</option>
                                <option value="2">Financiamiento Contado</option>
                                <option value="3">Otro Financiamiento</option>
                            </select>
                        </div>
                    </td>
                    <td width="60%">&nbsp;</td>
                </tr>
                <tr>
                    <td width="20%">
                        <img src="~/Content/Images/flecha.jpg" style="width:12px;height:12px" />&nbsp;<b><label class="labelBE">Segmento: </label></b>
                    </td>
                    <td width="30%">
                        <div class="custom-select" style="width: 90%; float: left">
                            <select id="cboSegmento" onchange="agregarFila()">
                                <option value="1" selected="selected">Corporativo</option>
                                <option value="2">Micro</option>
                                <option value="3">Pequeña</option>
                                <option value="4">Grandes Empresas</option>
                            </select>
                        </div>
                    </td>
                    <td width="60%">&nbsp;</td>
                </tr>
            </table>

            <table style="width: 100%; border-top: 5px">
                <tr>
                    <td colspan="3">
                        &nbsp;
                    </td>
                </tr>
                <tr>
                    <td colspan="5">
                        <img src="~/Content/Images/flecha.jpg" style="width:12px;height:12px" />&nbsp;<b><label class="labelBE">Tabla de Precios: </label></b>
                    </td>
                </tr>
                <tr>
                    <td width="8%">
                        <label class="labelBE2">Id Campaña :</label>
                    </td>
                    <td width="20%">
                        <input type="text" disabled value="0001" />
                    </td>
                    <td width="8%">
                        <label class="labelBE2">Nombre :</label>
                    </td>
                    <td width="20%">
                        <input type="text" disabled value="Campaña Verano 2020" size="30" />
                    </td>
                    <td width="44%"></td>
                </tr>
                <tr>
                    <td width="8%">
                        <label class="labelBE2">Fecha Inicio :</label>
                    </td>
                    <td width="20%">
                        <input type="text" disabled placeholder="19/02/2020" />
                    </td>
                    <td width="8%">
                        <label class="labelBE2">Fecha Fin :</label>
                    </td>
                    <td width="20%">
                        <input type="text" disabled placeholder="19/03/2020" />
                    </td>
                    <td width="44%"></td>
                </tr>
                <tr><td colspan="5"></td></tr>
                <tr><td colspan="5"></td></tr>
                <tr><td colspan="5"></td></tr>
                <tr>
                    <td colspan="5">
                        <table id="tblTarifado" style="width: 40%; border: 1px solid; border-color: darkgray">
                            <tr style="border: 1px solid; border-color: darkgray">
                                <td style="text-align: center; border: 1px solid; border-color: darkgray; background-color: lightgray; width: 19%;">
                                    <label class="labelBE2">Monto Transado</label>
                                </td>
                                <td style="text-align: center; border: 1px solid; border-color: darkgray; background-color: lightgray; width: 17%;">
                                    <label class="labelBE2">Costo SWIFT</label>
                                </td>
                                <td style="text-align: center; border: 1px solid; border-color: darkgray; background-color: lightgray; width: 17%;">
                                    <label class="labelBE2">Comisión</label>
                                </td>
                                <td id="td1" style="text-align: center; display: none; border: 1px solid; border-color: darkgray; background-color: lightgray; width: 17%;">
                                    <label class="labelBE2">Comisión Prorroga</label>
                                </td>
                                <td id="td2" style="text-align: center; display: none; border: 1px solid; border-color: darkgray; background-color: lightgray; width: 15%;">
                                    <label class="labelBE2">Spread Ejecutivo (%)</label>
                                </td>
                                <td id="td3" style="text-align: center; display: none; border: 1px solid; border-color: darkgray; background-color: lightgray; width: 15%;">
                                    <label class="labelBE2">Fecha Vencimiento Spread Ejecutivo</label>
                                </td>
                            </tr>
                        </table>
                    </td>
            </table>

            <table style="width: 100%; border-top: 5px">
                <tr>
                    <td colspan="3">
                        &nbsp;
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <button id="btnVolver" class="botonBE" onclick="javascript:window.history.back()">
                            Volver
                        </button>
                    </td>
                </tr>
            </table>
            
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")

    </center>

</body>
</html>
