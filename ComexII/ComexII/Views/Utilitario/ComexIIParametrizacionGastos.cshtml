@{
    ViewBag.Title = "Parametrización de gastos de envío";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=11" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="viewport" content="initial-scale=1.0, maximum-scale=2.0" />
    <link rel="shortcut icon" href="" />
    <meta name="description" content="Comex II - Parametrización de gastos de envío" />
    <meta name="author" content="Sermaluc S.A." />


    @Styles.Render("~/Content/css")

    <style type="text/css">
        .modal {
            display: none;
            position: fixed;
            z-index: 1000;
            top: 0;
            left: 0;
            height: 100%;
            width: 100%;
            background: #ACB4B9 url('/pu46/pux/ComexIIConsultas/Content/jqueryui1114/images/page-loader.gif') 50% 50% no-repeat;
            -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
        }

        body.loading .modal {
            overflow: hidden;
        }

        body.loading .modal {
            display: block;
        }

        .table2 {
            border: 1px solid #ACB4B9;
            background-color: #FFFFFF;
            font-family: Verdana;
            font-size: 9px;
        }

        .table3 {
            border: 0px;
            background-color: #FFFFFF;
            font-family: Verdana;
            font-size: 9px;
        }

        .TDBorder {
            border-color: #ACB4B9;
            border-style: solid;
            border-width: 1px;
        }
    </style>
</head>

<body>
    <center>
        <div id="wait" class="modal"></div>

        <div id="dialog-message" title="Parametrización de gastos de envío" style="display: none">
            <br />
            <strong>
                <label class="labelBE" id="lblMsg" style="text-align: center"></label>
            </strong>
        </div>

        <div id="dialog-error" title="Error" style="display: none">
            <br />
            <strong>
                <label class="labelBE2" id="lblError" style="text-align: center;">
                </label>
            </strong>
            <br />
            <div id="myDIV" style="display: none">
                <code id="error1"></code>
                <br />
                <code id="error2"></code>
                <br />
                <code id="error3"></code>
            </div>
        </div>

        <div id="divFiltrosODP">
            <table style="width: 100%">
                <tr>
                    <td>
                        <div class="Titulo1BE">
                            Parametrización de gastos de envío
                        </div>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <a href="#"><img src="~/Content/Images/print-icon.png" alt="Imprimir" height="20" width="20">&nbsp;imprimir</a>
                    </td>
                </tr>
            </table>

            <table style="width: 100%; border-top: 5px">
                <tr>
                    <td>
                        &nbsp;
                    </td>
                </tr>
                <tr>
                    <td colspan="5">
                        <img src="~/Content/Images/flecha.jpg" style="width:12px;height:12px" />&nbsp;<b><label class="labelBE">Gastos al exterior :</label></b>
                    </td>
                </tr>
                <tr>
                    <td width="20%">
                        <label class="labelBE2">Gastos BEN :</label>
                    </td>
                    <td width="20%">
                        <input type="text" style="text-align:right" value="10,00" size="15" />&nbsp;&nbsp;<b><label class="labelBE">USD</label></b>
                    </td>
                    <td width="60%">&nbsp</td>
                </tr>
                <tr>
                    <td width="20%">
                        <label class="labelBE2">Gastos OUR :</label>
                    </td>
                    <td width="20%">
                        <input type="text" style="text-align:right" value="20,00" size="15" />&nbsp;&nbsp;<b><label class="labelBE">USD</label></b>
                    </td>
                    <td width="60%">&nbsp</td>
                </tr>
                <tr>
                    <td width="20%">
                        <label class="labelBE2">Gastos OUR Garantizado :</label>
                    </td>
                    <td width="20%" colspan="2">
                        <label class="labelBE2">Monto igual o menor</label>&nbsp;&nbsp;&nbsp;&nbsp;<input type="text" style="text-align:right" value="120,00" size="15" />&nbsp;&nbsp;<b><label class="labelBE">USD</label></b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<label class="labelBE2">Paga</label>&nbsp;&nbsp;&nbsp;&nbsp;<input type="text" style="text-align:right" value="30,00" size="15" />&nbsp;&nbsp;<b><label class="labelBE">USD</label></b>
                    </td>
                </tr>
                <tr>
                    <td width="20%">
                        <label class="labelBE2"></label>
                    </td>
                    <td width="20%" colspan="4">
                        <label class="labelBE2">Monto igual o mayor</label>&nbsp;&nbsp;&nbsp;&nbsp;<input type="text" style="text-align:right" value="120,01" size="15" />&nbsp;&nbsp;<b><label class="labelBE">USD</label></b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<label class="labelBE2">Paga</label>&nbsp;&nbsp;&nbsp;&nbsp;<input type="text" style="text-align:right" value="50,00" size="15" />&nbsp;&nbsp;<b><label class="labelBE">USD</label></b>
                    </td>
                </tr>
            </table>

            <table style="width: 100%; border-top: 5px">
                <tr>
                    <td>
                        &nbsp;
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <button id="btnConfirmar" class="botonBE">
                            Confirmar
                        </button>
                    </td>
                </tr>
            </table>
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")

    </center>

</body>
</html>
